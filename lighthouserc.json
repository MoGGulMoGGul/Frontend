{
  "ci": {
    "collect": {
      "startServerCommand": "LHCI=true npm run start:3000",
      "startServerReadyPattern": "Local:\\s+http://localhost:3000",
      "startServerReadyTimeout": 120000,
      "puppeteerScript": "./scripts/puppeteer-login.cjs",
      "puppeteerScriptTimeout": 180000,
      "puppeteerLaunchOptions": {
        "args": ["--no-sandbox", "--disable-dev-shm-usage"]
      },
      "numberOfRuns": 3,
      "url": [
        "http://localhost:3000/login/",
        "http://localhost:3000/",
        "http://localhost:3000/challenge",
        "http://localhost:3000/edittip?id=87",
        "http://localhost:3000/mypage/",
        "http://localhost:3000/mytip/",
        "http://localhost:3000/newtip",
        "http://localhost:3000/newtip?storageNo=44&storageName=%EB%82%B4%EB%B3%B4%EA%B4%80%ED%95%A8",
        "http://localhost:3000/newtip?groupNo=5",
        "http://localhost:3000/search/?scope=all",
        "http://localhost:3000/signup",
        "http://localhost:3000/user?userNo=5",
        "http://localhost:3000/?modal=120",
        "http://localhost:3000/mytip/storage/?storageNo=3",
        "http://localhost:3000/grouptip/",
        "http://localhost:3000/grouptip/group/?groupNo=13",
        "http://localhost:3000/grouptip/storage/?groupNo=13&storageNo=44&storageName=%EC%8A%A4%ED%94%84%EB%A7%81%EB%B6%80%ED%8A%B8"
      ]
    },
    "assert": { "preset": "lighthouse:recommended" },
    "upload": { "target": "filesystem", "outputDir": ".lighthouse" }
  }
}
